        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Monthly_Points_Per_Child__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Additional monthly points per child in household.</inlineHelpText>
